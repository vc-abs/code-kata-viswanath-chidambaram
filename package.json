{
  "name": "npm-boilerplate",
  "version": "0.0.0",
  "description": "",
  "main": "./index.js",
  "scripts": {
    "start": "npm run build && npm run dev",
    "test": "jest --no-cache",
    "build": "rm -rf ./dist && babel --ignore '**/*.spec.js' -d ./dist ./src && npm run copy",
    "dev": "nodemon --exec babel-node src --unhandled-rejections=strict",
    "prepareDist": "npm run lint && npm test && npm run build",
    "dist": "npm run prepareDist && cd dist && npm publish --access public && cd ..",
    "copy": "cp ./package.json ./ReadMe.md ./LICENSE ./dist/",
    "lint": "eslint ."
  },
  "engines": {
    "node": ">=7.6"
  },
  "engineStrict": true,
  "repository": {
    "type": "git",
    "url": ""
  },
  "keywords": [],
  "author": "Laufire Technologies",
  "license": "ISC",
  "homepage": "",
  "devDependencies": {
    "@babel/cli": "^7.22.10",
    "@babel/core": "^7.22.10",
    "@babel/node": "^7.22.19",
    "@babel/plugin-transform-runtime": "^7.22.10",
    "@babel/preset-env": "^7.22.10",
    "@babel/runtime": "^7.22.10",
    "acorn": "^6.4.2",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^24.9.0",
    "eslint": "^5.16.0",
    "jest": "^24.9.0",
    "jest-cli": "^24.9.0",
    "nodemon": "^3.0.1",
    "pino-pretty": "^3.6.1",
    "regenerator-runtime": "^0.13.11"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "./src/**/*.js"
    ],
    "coverageDirectory": "./.coverage",
    "coverageThreshold": {
      "global": {
        "branches": 0,
        "functions": 0,
        "lines": 0,
        "statements": 0
      }
    },
    "resetMocks": true,
    "resetModules": true,
    "setupFilesAfterEnv": [
      "./test/setup/jest.setup.js"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/dist/"
    ],
    "transform": {
      "^.+\\.js?$": "babel-jest"
    }
  },
  "directories": {
    "example": "examples",
    "test": "test"
  },
  "dependencies": {
    "@laufire/utils": "3.0.0-0",
    "pino": "^5.17.0"
  }
}
